                var listobj = new rspPlaybackList("playbackList", 201, 20, "/HAPPLE/images/waiting.gif", "/HAPPLE/images/playback_process.jpg", "/HAPPLE/images/canner.png");
                jQuery(".playback-list-border").html(listobj.getHtml());
                listobj.setCancelFun("cancelPlayback");
				function cba()
				{
					ikp=0;
				}
				function pageAdd(){
					checkedNum = 0;
					pageNum=0;
					curn_page();
					$("#page").html(''+parseInt(parseInt(parseInt(pageNum))+1)+'');
					
					if(checkedNum == 20)
					{
						document.getElementById("checkAll").checked = true;
					}
					else
					{
						document.getElementById("checkAll").checked = false;
					}
				}
				function pageNext(){
					checkedNum = 0;
					if(parseInt(parseInt(pageNum)+1)<parseInt(rsp_getCookie("pageCount"))){
						pageNum=parseInt(parseInt(pageNum)+1);
						curn_page();
						$("#page").html(''+parseInt(parseInt(pageNum)+1)+'');
					}
					if(checkedNum == 20)
					{
						document.getElementById("checkAll").checked = true;
					}
					else
					{
						document.getElementById("checkAll").checked = false;
					}
				}
				function pageUp(){
					checkedNum = 0;
					if(parseInt(parseInt(pageNum)-1)>=0){
						pageNum=parseInt(pageNum-1);
						curn_page();
						$("#page").html(''+parseInt(parseInt(pageNum)+1)+'');
					}
					
					if(checkedNum == 20)
					{
						document.getElementById("checkAll").checked = true;
					}
					else
					{
						document.getElementById("checkAll").checked = false;
					}
				}
				function pageLast(){
					checkedNum = 0;
					if(rsp_getCookie("pageCount") == "")
					{
						pageNum = 0;
					}
					else
					{
						pageNum=parseInt(parseInt(rsp_getCookie("pageCount"))-1);
					}
					curn_page();
					$("#page").html(''+parseInt(parseInt(pageNum)+1)+'');
					
					if(checkedNum == 20)
					{
						document.getElementById("checkAll").checked = true;
					}
					else
					{
						document.getElementById("checkAll").checked = false;
					}
				}
            </script>
        </div>
	</div>
	<div class="clear"></div>
</div>


