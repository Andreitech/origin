            <script type="text/javascript">
				 if ('CUSTOMER_SAFARI' == 'CUSTOMER_CALSYS'  || 'CUSTOMER_SAFARI' == 'CUSTOMER_CALSYS_NEUTRAL'){
					  document.getElementById("pageF").style.color = "#66F"
