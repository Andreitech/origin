			<script type="text/javascript">

			new rspButton({ id: "btn_liveView", name: "btn_liveView", text: "Наблюдение", height: 33, bgimg: "/HAPPLE/images/menubutton.jpg?"+RM.SYS.custName, onclick: 'liveViewClick()' });

		function liveViewClick()
		{
			if(window.document.URL.split("#")[1]!="/liveView.rsp")
			{
				RM.frame.loadPage("/liveView.rsp"); 
			}
			jQuery("#btn_liveView").css("color","#01CDFF");
			jQuery("#btn_configure").css("color","black");
			jQuery("#btn_playback").css("color","black");
			
			
			if(RM.SYS.priwe == "true" && rsp_getCookie("startOrStop") !=  "false")
			{
				document.getElementById("startOrStopview").value="true";
				document.getElementById("startOrStopview").className="content_right_bottom_image_down_13";
				document.getElementById("MainStream").disabled=false;
				document.getElementById("SonStream").disabled=false;
			}
			else
			{
				document.getElementById("startOrStopview").value="false";
				document.getElementById("startOrStopview").className="content_right_bottom_image_13";
				if( rsp_getCookie("startOrStop") != "true" )
				{
					document.getElementById("MainStream").disabled=true;
					document.getElementById("SonStream").disabled=true;
				}
			}
			
			if( rsp_getCookie("startOrStop") == "true" )
			{
				var streaming=rsp_getCookie("EncodingVlaue");
				if (streaming == '') { streaming = 0; };
				//RM.frame.getDevice().startAllPreview(parseInt(streaming));
				$("#startOrStopview").attr("class","content_right_bottom_image_down_13");
				$("#startOrStopview").attr("value","true");
			}
			
			
			
			setTimeout(function(){
				if(rsp_getCookie("scren") != "true")
				{
					//document.getElementById("dvrplug").StopRecord(RM.frame.getDevice().gethandle(),0);
					document.getElementById("Fullscreen").className="content_right_bottom_image_2";
					document.getElementById("Fullscreen").value="false";
					rsp_setCookie("scren","false");
				}
				else
				{
					//document.getElementById("dvrplug").StartRecord(RM.frame.getDevice().gethandle(),0,0);
					//document.getElementById("Fullscreen").className="content_right_bottom_image_down_2";
					document.getElementById("Fullscreen").value="true";
					rsp_setCookie("scren","true");
				}
			},1000);
			resizeViewWindow();
		}
		
		if(window.document.URL.split('#')[1]=="/liveView.rsp"||window.document.URL.split('#')[1]==undefined){
            jQuery("#btn_liveView").css("color","#01CDFF");
		}
            </script>
