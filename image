<script type="text/javascript">
    function loadDefaultParam() {
        var defaultParam = new rspConfigure("sys_default_param", RM.SYS.MAX_CHL_NUM);
        defaultParam.getDefaultParamAllFromDevice();
    }
    function loadPage() {
        var obj = document.getElementById("dvrplug");
		if(obj && typeof(obj.Login) != 'undefined' &&!RM.frame.getDevice()){
            setTimeout("loadPage()",100);
            return;
        }
        setTimeout("loadDefaultParam()", 2000);
        var curPage = '/liveView.rsp';
        var pages = window.document.URL.split("#");
        if (pages.length > 1) {
            curPage = pages[1];
        }
		if(curPage == "/left.rsp")
		{
			jQuery.fn.sysTips({title:RM.language.SystemTips,tips:RM.language.LoadingParameter,type:'-1'});
		}
        RM.frame.loadPage(curPage);
        
    }
