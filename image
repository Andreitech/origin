<script type="text/javascript">
    var locationUrl = window.location.href;
    if (locationUrl.indexOf("islogout=0") > 0) {
        document.getElementById("btn_head_logout").style.display = 'none';
    }
    if (locationUrl.indexOf("showheader=0") > 0) {
        document.getElementById("head-border").style.display = 'none';
    }
	//plug param
	var customId = "0";
	if( customId == "" )
	{
		customId = 0;
	}
	else
	{
		customId = parseInt(customId);
	}
	document.getElementById("dvrplug").SetClientID(customId);
	var url = window.document.URL;
	var por = url.split(':');
	
	if(por[2] != undefined)
	{
		var port;
		if(por[2].indexOf('/')>0)
		{
			port = por[2].substring(0,por[2].indexOf('/'));
		}
		else
		{
			port = por[2];
		}
	
		if(!isNaN(port))
		{
			document.getElementById("dvrplug").SetWebport(port);
		}
	}
	
	function clearCookie_Encoding()
	{rsp_setCookie("EncodingVlaue","");}
	
	if(RM.SYS.custName != "CUSTOMER_NULL")
	{
		$(".head-plug-reserver").hide();
	}
</script>

