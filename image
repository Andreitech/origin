						  function selectMaxForBy7116(id){
							    checkeCount=0;
								if(RM.SYS.product_type.split("_")[0]=="7116XE" || RM.SYS.product_type.split("_")[0]=="7116XC" || RM.SYS.product_type == "8632XH_VA"|| RM.SYS.product_type == "8832XH_VA"|| RM.SYS.product_type == "9208XH_VA"|| RM.SYS.MAX_CHL_NUM == 32){
									var chex = $("#playback_channel_border input[type='checkbox']");
									for(var i=0;i<chex.length;i++){
										if(chex[i].checked ){
											if(chex[i].name != 'all')
											{
												checkeCount++;
											}
										}
									}
									if(checkeCount >= 4){
										if(checkeCount>4){
											document.getElementById(id).checked=false;	
											if( document.getElementById(id).name == 'all')
											{
												var arr = id.split("_");
												checkedCHl(parseInt(arr[1]));
												return;
											}
										}
										var chebx = $("#playback_channel_border input[type='checkbox']");
										for(var i=0;i<chebx.length;i++){
											if(!(chebx[i].checked)){
												chebx[i].disabled = true;
											}
										}
									}else{
										var chebx = $("#playback_channel_border input[type='checkbox']");
										for(var i=0;i<chebx.length;i++){
											if(!(chebx[i].checked)){
												chebx[i].disabled = false;
											}
										}
									}
								}
						  }
						  var count=0;
						  var flag = 0;
						  var maxNum = 0;
						  var tcount=0;
						  var tflag = 0;
						  var tmaxNum = 0;
						  var allFlag = 0;
						  var gFlag = 3;
						  function isAllChecked(num){
								if((num<=3) || (num > 3 && num <=7) || (num>7 && num <= 11) || (num >11 && num <= 15) || (num>15 && num <= 19) || (num>19 && num <= 23) || (num>23 && num <= 27) || num > 27){
									count = 0;
									if(num <= 3){
										maxNum = 3;
										flag = 0;
									}else if(num > 3 && num <= 7){
										flag = 4;
										maxNum = 7;
									}else if(num > 7 && num <= 11){
										flag = 8;
										maxNum = 11;
									}else if(num > 11 && num <= 15){
										flag = 12;
										maxNum = 15;
									}else if(num > 15 && num <= 19){
										flag = 16;
										maxNum = 19;
									}else if(num > 19 && num <= 23){
										flag = 20;
										maxNum = 23;
									}else if(num > 23 && num <= 27){
										flag = 24;
										maxNum = 27;
									}else if(num > 27){
										flag = 28;
										maxNum = 31;
									}else{
									}
								}
								for( var i=flag; i<=maxNum;i++){
									if(document.getElementById("playback_sel_ch"+i+"").checked){
										count++;
									}else{
									}
								}
								if(count != 0 && count%4 == 0){
										document.getElementById("checkedall_"+flag+"").checked = true;
									}else{
										document.getElementById("checkedall_"+flag+"").checked = false;
									}
						  }

						var dcout = 0 ;
						function checked_7116XE(num){
							dcout = tcount;
							if((num<=3) || (num>3 && num<=7) || (num>7 && num<=11) || (num>11 && num<=15) || (num>15 && num<=19) || (num>19 && num<=23) || (num>23 && num<=27) || num>27){
									tcount = 0;
									if(num <= 3){
										tmaxNum = 3;
										tflag = 0;
									}else if(num > 3 && num <= 7){
										tflag = 4;
										tmaxNum = 7;
									}else if(num > 7 && num <= 11){
										tflag = 8;
										tmaxNum = 11;
									}else if(num > 11 && num <= 15){
										tflag = 12;
										tmaxNum = 15;
									}else if(num > 15 && num <= 19){
										tflag = 16;
										tmaxNum = 19;
									}else if(num > 19 && num <= 23){
										tflag = 20;
										tmaxNum = 23;
									}else if(num > 23 && num <= 27){
										tflag = 24;
										tmaxNum = 27;
									}else if(num > 27){
										tflag = 28;
										tmaxNum = 31;
									}else{
									}
								}
								if(gFlag != tmaxNum){
									gFlag = tmaxNum;
									allFlag = 1;
								}else{
										allFlag = 0;
								}
								for( var i=tflag; i<=tmaxNum;i++){
									if(document.getElementById("playback_sel_ch"+i+"").checked){
										tcount++;
									}else{
									}
								}
								if(tcount == 4){
									for(var i=0; i<RM.SYS.MAX_CHL_NUM;i++){
										if(i<tflag ||  i>tmaxNum){
											document.getElementById("playback_sel_ch"+i+"").disabled = true;
											}
									}
									if((tflag-4) >= 0){
										document.getElementById("checkedall_"+(tflag-4)+"").disabled = true;
									}else if((tflag+4) < RM.SYS.MAX_CHL_NUM){
										document.getElementById("checkedall_"+(tflag+4)+"").disabled = true;
									}else{}
								}else if(tcount>0){
									for(var i=0; i<RM.SYS.MAX_CHL_NUM;i++){
										if(i<tflag ||  i>tmaxNum){
											document.getElementById("playback_sel_ch"+i+"").disabled = false;
											}
									}
									if((tflag-4) >= 0){
										document.getElementById("checkedall_"+(tflag-4)+"").disabled = true;
									}else if((tflag+4) < RM.SYS.MAX_CHL_NUM){
										document.getElementById("checkedall_"+(tflag+4)+"").disabled = true;
									}else{}
								}else{
									
								}
								
								/*for(var j = 0; j < RM.SYS.MAX_CHL_NUM;j++){
										if(j == 0 || j == 4 || j == 8 || j == 12 || j == 16 || j == 20 || j == 24 || j == 28){
											document.getElementById("checkedall_"+j+"").diabled = false;
										}
									}*/
						}

						  function checkedCHl(num)
						  {
								if(RM.SYS.product_type.split("_")[0]=="7116XE" || RM.SYS.product_type.split("_")[0]=="7116XC" || RM.SYS.product_type == "8632XH_VA"|| RM.SYS.product_type == "8832XH_VA"|| RM.SYS.product_type == "9208XH_VA"|| RM.SYS.MAX_CHL_NUM == 32){
									if(checkeCount != "0" && allFlag == 1){
										document.getElementById("checkedall_"+num+"").checked = false;
										return;
									}
								}
						  		if( document.getElementById("checkedall_"+num+"").checked )
								{
									for( var i=num; i<(num+4);i++)
									{
										//selectMaxForBy7116("playback_sel_ch"+i+"");
										document.getElementById("playback_sel_ch"+i+"").checked = true;
									}
								}
								else
								{
									for( var i=num; i<(num+4);i++)
									{
										document.getElementById("playback_sel_ch"+i+"").checked = false;
									}
								}
						  }
