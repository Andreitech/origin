         <script type="text/javascript">
		 if(RM.SYS.talkback != "true")
		 {
			document.getElementById("duijiang").style.display="none";
		 }
		
		if(RM.SYS.priwe == "true")
		{
			document.getElementById("startOrStopview").value="true";
			document.getElementById("startOrStopview").className="content_right_bottom_image_down_13 png";
			setTimeout(function(){$("#MainStream").removeAttr("disabled");
			$("#SonStream").removeAttr("disabled");},2000);
		}
		else
		{
			document.getElementById("startOrStopview").value="false";
			document.getElementById("startOrStopview").className="content_right_bottom_image_13 png";
			setTimeout(function(){document.getElementById("MainStream").disabled=true;
			document.getElementById("SonStream").disabled=true;},2000);
		}
			
		if(RM.SYS.product_type.split("_")[0]=="9204XH" || RM.SYS.product_type.split("_")[0]=="9204XD" || RM.SYS.product_type.split("_")[0]=="9208XH")
		{
			$(".left_bottom_bottom").hide();
		}
		
			function MainStream_over_down(id,cv_value){
					document.getElementById("SonStream").className="content_right_bottom_image_5";
					document.getElementById("SonStream").value="false";
					document.getElementById("MainStream").className="content_right_bottom_image_down_4";
					document.getElementById("MainStream").value="true";
					document.getElementById('dvrplug').SwitchPreviewStreamType(RM.frame.getDevice().gethandle(),1);
					
					rsp_setCookie("EncodingVlaue",1);
			}
			
			function SonStream_down(id,cv_value){  //开启子码流
					document.getElementById("MainStream").className="content_right_bottom_image_4";	
					document.getElementById("MainStream").value="false";
					
					document.getElementById("SonStream").className="content_right_bottom_image_down_5";
					document.getElementById("SonStream").value="true";
					document.getElementById('dvrplug').SwitchPreviewStreamType(RM.frame.getDevice().gethandle(),0);
					rsp_setCookie("EncodingVlaue",0);
			}
				
			function setFullscreen(id,value){
				value=$("#"+id+"").attr("value");
				var closeOral=$("#startOrStopview").attr("value");
				if(value=="false"&&closeOral=="true"){
					
					document.getElementById("dvrplug").StartRecord(RM.frame.getDevice().gethandle(),0,0);
					document.getElementById(id).value="true";
					rsp_setCookie("scren","true");
				}
				else
				{
					document.getElementById("dvrplug").StopRecord(RM.frame.getDevice().gethandle(),0);
					document.getElementById(id).value="false";
					rsp_setCookie("scren","false");
				}

			}
			
			function setTalkback(id,value){
				value=$("#"+id+"").attr("value");
				//var closeOral=$("#startOrStopview").attr("value");
				if(value=="false"){
					document.getElementById(id).disabled = true;
					var callbackValue = document.getElementById("dvrplug").Talk(RM.frame.getDevice().gethandle(),1);
					if(callbackValue == 0)
					{
						document.getElementById(id).disabled = false;
						document.getElementById(id).value="true";
					}
					else
					{
						if(callbackValue == 6)
						{
							alert(RM.language.IE_DEV_OFFLINE);
						}
						else if(callbackValue == 67)
						{
							alert(RM.language.IE_TALK_AUDIO_ENCODE_FALED);
						}
						else if(callbackValue == 67)
						{
							alert(RM.language.IE_TALK_AUDIO_DECODE_FALED);
						}
						else if(callbackValue == 69)
						{
							alert(RM.language.IE_TALK_START_FALED);
						}
						
						document.getElementById(id).disabled = false;
					}
				}
				else
				{
					document.getElementById(id).disabled = true;
					var callbackValue = document.getElementById("dvrplug").Talk(RM.frame.getDevice().gethandle(),0);
					if(callbackValue == 0)
					{
						document.getElementById(id).disabled = false;
						document.getElementById(id).value="false";
					}
					else
					{
						if(callbackValue == 6)
						{
							alert(RM.language.IE_DEV_OFFLINE);
						}
						else if(callbackValue == 67)
						{
							alert(RM.language.IE_TALK_AUDIO_ENCODE_FALED);
						}
						else if(callbackValue == 67)
						{
							alert(RM.language.IE_TALK_AUDIO_DECODE_FALED);
						}
						else if(callbackValue == 69)
						{
							alert(RM.language.IE_TALK_START_FALED);
						}
						document.getElementById(id).disabled = false;
					}
				}

			}
			
			function startOrStopviews(id,value){
				if(value=="true") {
						rsp_setCookie("startOrStop","false");
			            RM.frame.getDevice().stopPreviewAll();
						$("#"+id+"").attr("class","content_right_bottom_image_13");
						$("#"+id+"").attr("value","false");
						document.getElementById("dvrplug").StopRecord(RM.frame.getDevice().gethandle(),0);
						document.getElementById("Fullscreen").className="content_right_bottom_image_2";
						document.getElementById("Fullscreen").value="false";
						document.getElementById("MainStream").disabled=true;
						document.getElementById("SonStream").disabled=true;
						
						rsp_setCookie("scren","false");
						
				}else{
					rsp_setCookie("startOrStop","true");
					var streaming=rsp_getCookie("EncodingVlaue");
					if (streaming == '') { streaming = 0; };
					RM.frame.getDevice().startAllPreview(parseInt(streaming));
					$("#"+id+"").attr("class","content_right_bottom_image_down_13");
					$("#"+id+"").attr("value","true");
					document.getElementById("MainStream").disabled=false;
					document.getElementById("SonStream").disabled=false;
					
					//document.getElementById("dvrplug").StartRecord(RM.frame.getDevice().gethandle(),0,0);
					//document.getElementById(id).value="true";
					//rsp_setCookie("scren","true");
				}
			}
			
			function reviewDig_click(id,value)
			{
				if(value=="true") {
					$("#"+id+"").attr("class","content_right_bottom_image_14");
					$("#"+id+"").attr("value","false");
					document.getElementById("dvrplug").PreviewDigitalRoomControl(RM.frame.getDevice().gethandle(),0);
				}
				else
				{
					$("#"+id+"").attr("class","content_right_bottom_image_down_14");
					$("#"+id+"").attr("value","true");
					document.getElementById("dvrplug").PreviewDigitalRoomControl(RM.frame.getDevice().gethandle(),1);
				}
			}
        </script>

        
         <li style="float:left;margin-left:10px; margin-top:2px;">
    		 <div class="content_right_bottom_image_6 png" id="yinL" value="false" onmouseover="ChangeViewBtnBg(0,'yinL','content_right_bottom_image_onmouse_6')" 
        onmouseout="ChangeViewBtnBg(1,'yinL','content_right_bottom_image_6')"  onclick="VolumSet()" 
         title="Выкл. звук">
        </div>
   	    </li>
        	<script>
			//rsp_setCookie("volume",jQuery("#yinL").attr("class"));
			if(volume!=0){
			if(rsp_getCookie("volume")==""){rsp_setCookie("volume",jQuery("#yinL").attr("class").split(' ')[0]);};
			document.getElementById("yinL").className=rsp_getCookie("volume");
			document.getElementById("yinL").value="true";index_windowTure++;
			}else{volume++;}
			
			function VolumSet(){
				if(rsp_getCookie("vol")=="false"){
					$("#yinL").attr("value","false");
					document.getElementById("dvrplug").SetPreviewSoundState(RM.frame.getDevice().gethandle(),1);
					rsp_setCookie("vol","true");
					rsp_setCookie("volume","content_right_bottom_image_6");
					$("#yinL").attr("class","content_right_bottom_image_6");
				}else{
					$("#yinL").attr("value","true");
					document.getElementById("dvrplug").SetPreviewSoundState(RM.frame.getDevice().gethandle(),0);
					rsp_setCookie("vol","false");
					rsp_setCookie("volume","content_right_bottom_image_down_6");
					$("#yinL").attr("class","content_right_bottom_image_down_6");
				}
			}
			function setPreviewValue(id){
				var bol;
				if( RM.SYS.custName == "CUSTOMER_NIGHTOWL" || RM.SYS.custName == "CUSTOMER_QSEE" )
				{
					bol = document.getElementById(id).value;
				}
				else
				{
					 bol=rsp_getCookie("maxbol");
				}
				if (bol == "false")
				{
					document.getElementById(id).value="true";
					document.getElementById(id).className="content_right_bottom_image_onmouse_18_1";
				
					if( RM.SYS.custName == "CUSTOMER_NIGHTOWL" || RM.SYS.custName == "CUSTOMER_QSEE" )
					{
						document.getElementById("dvrplug").SetPreviewVideoShapeMode(1);
					}
					else
					{
						rsp_setCookie("maxbol","true");
						document.getElementById("dvrplug").SetPlayBackWindowShapeState(RM.frame.getDevice().gethandle(),1,0);
					}
				}
				else
				{
					document.getElementById(id).value="false";
					document.getElementById(id).className="content_right_bottom_image_onmouse_18";
					
					if( RM.SYS.custName == "CUSTOMER_NIGHTOWL" || RM.SYS.custName == "CUSTOMER_QSEE" )
					{
						document.getElementById("dvrplug").SetPreviewVideoShapeMode(0);
					}
					else
					{
						rsp_setCookie("maxbol","false");
						document.getElementById("dvrplug").SetPlayBackWindowShapeState(RM.frame.getDevice().gethandle(),1,1);
					}
				}
				if( RM.SYS.custName != "CUSTOMER_NIGHTOWL" && RM.SYS.custName != "CUSTOMER_QSEE" )
				{
					resizeViewWindow();
					RM.frame.changeUrl("liveView.rsp",1);
				}
			}
		</script>
        
