	<script type="text/javascript">
		
		
		var numk = 0;
	    function UploadBackupProcess(BackupKey, ulFlag, TotalLen, CurLen, percent) {
	        RM.frame.getDevice().backupProcess(BackupKey, ulFlag, percent);
	    }
		function ReTurnPreviewActiveChl(ulActiveChannel){
			if(RM.frame.getDevice().getActiveChannel()>=0&&RM.frame.getDevice().getActiveChannel()<=RM.SYS.MAX_CHL_NUM){
				refer_value(2);
			}
			if(RM.frame.getDevice().getActiveChannel() <= 3 && RM.SYS.product_type == "9216XH_VA")
			{
				 $("#diabledBydisplay div").attr("disabled","true");
				$("#ytspeed").attr("disabled","true");
				$("#ytspeedRemove").attr("disabled","true");
			 }else{
				$("#diabledBydisplay div").removeAttr("disabled");
				$("#ytspeed").removeAttr("disabled");
				$("#ytspeedRemove").removeAttr("disabled");
			 }
		}
		
		
		function ReTurnPreviewAllChannelState(ulforcedlogout, ulstate) {
		
			if( ulforcedlogout == 0 )
			{
				if( ulstate == 0 )
				{
					// RM.frame.getDevice().stopPreviewAll();
						//document.getElementById("dvrplug").StopPreviewAll(RM.frame.getDevice().gethandle());
						$("#startOrStopview").attr("class","content_right_bottom_image_13");
						$("#startOrStopview").attr("value","false");
						//document.getElementById("dvrplug").StopRecord(RM.frame.getDevice().gethandle(),0);
						document.getElementById("Fullscreen").className="content_right_bottom_image_2";
						document.getElementById("Fullscreen").value="false";
						document.getElementById("MainStream").disabled=true;
						document.getElementById("SonStream").disabled=true;
						rsp_setCookie(" startOrStop","false");
				}
				else
				{
					$("#startOrStopview").attr("class","content_right_bottom_image_down_13");
					$("#startOrStopview").attr("value","true");
					document.getElementById("MainStream").disabled=false;
					document.getElementById("SonStream").disabled=false;
					
					rsp_setCookie(" startOrStop","true");
				}
			}
			else if( ulforcedlogout == 1 )
			{
				if( ulstate==0 )
				{
					//ocument.getElementById("dvrplug").StopRecord(RM.frame.getDevice().gethandle(),0);
					document.getElementById("Fullscreen").className="content_right_bottom_image_2";
					document.getElementById("Fullscreen").value="false";
				}
				else
				{
					//document.getElementById("dvrplug").StartRecord(RM.frame.getDevice().gethandle(),0,0);
					
						document.getElementById("Fullscreen").className="content_right_bottom_image_down_2";
						document.getElementById("Fullscreen").value="true";
				}
			}
		    else if (ulforcedlogout == 2) {
		        RM.frame.getDevice().logout();
		        alert("" + RM.language.IE_BEFORCED_OFFLINE + "");
		        window.location = "/login.rsp";
		    }
		    else if (ulforcedlogout == 3) {
		        if (ulstate == 1 && RM.frame.getDevice().getStreamType() != 1 ){
		            document.getElementById("SonStream").className = "content_right_bottom_image_5";
		            document.getElementById("SonStream").value = "false";
		            document.getElementById("MainStream").className = "content_right_bottom_image_down_4";
		            document.getElementById("MainStream").value = "true";
		            RM.frame.getDevice().changeStreamTypeByDevice(1);
		        }
		        if (ulstate == 0 && RM.frame.getDevice().getStreamType() != 0) {
		            document.getElementById("MainStream").className = "content_right_bottom_image_4";
		            document.getElementById("MainStream").value = "false";
		            document.getElementById("SonStream").className = "content_right_bottom_image_down_5";
		            document.getElementById("SonStream").value = "true";
		            RM.frame.getDevice().changeStreamTypeByDevice(0);
		        }
		    }
			
			else if(ulforcedlogout == 4)
			{
				$("#reviewDig").attr("class","content_right_bottom_image_14");
				$("#reviewDig").attr("value","false");
				document.getElementById("dvrplug").PreviewDigitalRoomControl(RM.frame.getDevice().gethandle(),0);
			}
			else if( ulforcedlogout == 5 )
			{
				document.getElementById('Talkback').value = 'false';
				document.getElementById('Talkback').className = 'content_right_bottom_image_talkback';
			}
			else if( ulforcedlogout == 6 )
			{
				if( ulstate == 0 )
				{
					document.getElementById("setPreview").value="false";
					document.getElementById("setPreview").className="content_right_bottom_image_onmouse_18";
					rsp_setCookie("maxbol","false");
				}
				else if( ulstate == 1 )
				{	
					document.getElementById("setPreview").value="true";
					document.getElementById("setPreview").className="content_right_bottom_image_onmouse_18_1";
					rsp_setCookie("maxbol","true");
				}
				a = 0;
				resizePlaybackWindow();
				RM.frame.changeUrl("/playback.rsp",1);
			}
		}
	    RM.box = new rspBox("g_box", "g_box", 0, "/HAPPLE/images/rspbox_border_bg.gif", "/HAPPLE/images/rspbox_content_bg.gif", "/HAPPLE/images/rsp_box_title_bg.jpg");
	</script>
